                                            <Binding Path="DSMAX" UpdateSourceTrigger="PropertyChanged" Converter="{StaticResource UnsetValueConverter}"/>
                                        </MultiBinding>
                                    </Label.Content>
                                </Label>
                                <Label Grid.Row="9" Grid.Column="2" Grid.ColumnSpan="2" Name="Change_Line" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden" >
                                    <Label.Content>
                                        <MultiBinding  Converter="{StaticResource changeconverter}">
                                            <Binding Path="T" UpdateSourceTrigger="PropertyChanged" Converter="{StaticResource UnsetValueConverter}"/>
                                            <Binding Path="W1" UpdateSourceTrigger="PropertyChanged" Converter="{StaticResource UnsetValueConverter}"/>
                                            <Binding Path="W2" UpdateSourceTrigger="PropertyChanged" Converter="{StaticResource UnsetValueConverter}"/>
                                        </MultiBinding>
                                    </Label.Content>
                                </Label>
                                <Label Grid.Row="9" Grid.Column="2" Grid.ColumnSpan="2" Name="Delta_Line" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden" >
                                    <Label.Content>
                                        <MultiBinding  Converter="{StaticResource deltaconverter}">
                                            <Binding Path="DeltaT" UpdateSourceTrigger="PropertyChanged" Converter="{StaticResource UnsetValueConverter}"/>
                                            <Binding Path="DeltaW1" UpdateSourceTrigger="PropertyChanged" Converter="{StaticResource UnsetValueConverter}"/>
                                            <Binding Path="DeltaW2" UpdateSourceTrigger="PropertyChanged" Converter="{StaticResource UnsetValueConverter}"/>
                                        </MultiBinding>
                                    </Label.Content>
                                </Label>
                                <Label Grid.Row="6" Grid.Column="2" Name="Input_X1_Line" Content="{Binding X1, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden"></Label>
                                <Label Grid.Row="6" Grid.Column="3" Name="Input_X2_Line" Content="{Binding X2, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden"></Label>
                                <Label Grid.Row="7" Grid.Column="2" Grid.ColumnSpan="2" Name="Input_Y_Line" Content="{Binding Y, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden"></Label>
                                <Label Grid.Row="8" Grid.Column="2" Grid.ColumnSpan="2" Name="Entscheidung" HorizontalAlignment="Center"></Label>
                                <Label Grid.Row="8" Grid.Column="4" Content="{DynamicResource Fehler_Titel}" HorizontalAlignment="Right" FontWeight="Bold" FontSize="14"/>
                                <Label Grid.Row="8" Grid.Column="5" Content="{Binding Error, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Center" VerticalAlignment="Center"></Label>
                            </Grid>
                            <GroupBox Grid.Column="1" Header="Optionen">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="8*"/>
                                        <RowDefinition Height="7*"/>
                                        <RowDefinition Height="1*"/>
                                    </Grid.RowDefinitions>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="1*"/>
                                            <RowDefinition Height="2*"/>
                                            <RowDefinition Height="2*"/>
                                            <RowDefinition Height="2*"/>
                                            <RowDefinition Height="2*"/>
                                            <RowDefinition Height="2*"/>
                                            <RowDefinition Height="1*"/>
                                        </Grid.RowDefinitions>
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="4*"/>
                                                <ColumnDefinition Width="1*"/>
                                            </Grid.ColumnDefinitions>
                                            <Label Grid.Column="0" Content="{DynamicResource Runden_Titel}" VerticalAlignment="Center" HorizontalAlignment="Center"></Label>
                                            <Slider Grid.Column="1" Maximum="10" IsSnapToTickEnabled="True" Name="RndValue" Minimum="2" SmallChange="1" Value="5" ValueChanged="RndValue_ValueChanged" VerticalAlignment="Center" />
                                            <TextBox Grid.Column="2" Text="{Binding ElementName=RndValue, Path=Value, UpdateSourceTrigger=PropertyChanged}" TextAlignment="Right" Width="30" VerticalAlignment="Center" />
                                        </Grid>
                                        <Grid Grid.Row="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="4*"/>
                                                <ColumnDefinition Width="1*"/>
                                            </Grid.ColumnDefinitions>
                                            <Label Grid.Column="0" Content="{DynamicResource LP_Titel}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                            <Slider Grid.Column="1" Maximum="0.50" IsSnapToTickEnabled="True" Name="ErrorValue" Minimum="0" SmallChange="0.05" Value="0.15"  VerticalAlignment="Center" ValueChanged="LPValue_ValueChanged" Ticks="0.05 0.10 0.15 0.20 0.25 0.30 0.35 0.40 0.45" />
                                            <TextBox Grid.Column="2" Text="{Binding ElementName=ErrorValue, Path=Value, UpdateSourceTrigger=PropertyChanged}" TextAlignment="Right" Width="30" VerticalAlignment="Center" />
                                        </Grid>
                                        <Grid Grid.Row="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="1*"/>
                                            </Grid.ColumnDefinitions>
                                            <RadioButton Name="IsOnline" Grid.Column="0" IsChecked="True" HorizontalAlignment="Center">Online-Training</RadioButton>
                                            <RadioButton Grid.Column="1" HorizontalAlignment="Center">Batch-Training</RadioButton>
                                        </Grid>
                                        <Grid Grid.Row="4">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="2*"/>
                                            </Grid.ColumnDefinitions>
                                            <Label Grid.Column="0" Content="X1:" HorizontalAlignment="Center"></Label>
                                            <TextBox Name="SWE_X1_Input" Grid.Column="1"></TextBox>
                                            <Label Grid.Column="2" Content="X2:" HorizontalAlignment="Center"></Label>
                                            <TextBox Name="SWE_X2_Input" Grid.Column="3"></TextBox>
                                            <Label Grid.Column="4" Content="Y:" HorizontalAlignment="Center"></Label>
                                            <TextBox Name="SWE_Y_Input" Grid.Column="5"></TextBox>
                                        </Grid>
                                        <Grid Grid.Row="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="2*"/>
                                            </Grid.ColumnDefinitions>
                                            <Button Name="Uebung_1_Add_Button" Grid.Column="3" Content="{DynamicResource Add_Std_Button}" Margin="0,5,0,0"/>
                                            <Button Grid.Column="5" Content="{DynamicResource Add_Button}" Margin="0,5,0,0" Click="Add_Schwellenwert_Input"/>
                                        </Grid>
                                    </Grid>
                                    <DataGrid Grid.Row="1" Name="Uebung_1_Input" AlternatingRowBackground="BlanchedAlmond" AutoGenerateColumns="False">
                                        <DataGrid.Columns>
                                            <DataGridTemplateColumn Header="X1" Width="50">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBox Text="{Binding X1}" BorderBrush="{x:Null}" Background="{x:Null}" />
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn Header="X2" Width="50">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBox Text="{Binding X2}" BorderBrush="{x:Null}" Background="{x:Null}" />
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn Header="Y" Width="50">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBox Text="{Binding Y}" BorderBrush="{x:Null}" Background="{x:Null}" />
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <DataGridTemplateColumn Header="{DynamicResource entfernen}" Width="100">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Button Content="X" Tag="{Binding}" Click="Delete_Line" HorizontalAlignment="Center"/>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                        </DataGrid.Columns>
                                    </DataGrid>
                                    <Grid Grid.Row="2">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="1*"/>
                                            </Grid.ColumnDefinitions>
                                            <Button Grid.Column="0" Content="Training" Background="#FF62E418" Click="Schwellenwert_Training"></Button>
                                            <Button Grid.Column="1" Content="Clear" Background="#FFE46218" Click="Schwellenwert_Clear"></Button>
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </GroupBox>
                        </Grid>
                    </Grid>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <Button Name="Uebung_1_back" Grid.Column="0" FontSize="14" Background="{x:Null}" BorderThickness="0,2,3,0" Click="tab_back"></Button>
                        <Label Name="Uebung_1_label" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"></Label>
                        <Button Name="Uebung_1_forward" Grid.Column="2" FontSize="14" Background="{x:Null}" BorderThickness="3,2,0,0" Click="tab_forward"></Button>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Visibility="Collapsed">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="30"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0" Content="{DynamicResource Uebung_1_Untertitel_2}" FontSize="14" Background="{x:Null}" BorderThickness="0,2,3,0" Click="tab_back"></Button>
                        <Label Grid.Column="1" Content="{DynamicResource Extra_1_Untertitel_1}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"></Label>
                        <Button Grid.Column="2" Content="{DynamicResource Extra_1_Untertitel_2}" FontSize="14" Background="{x:Null}" BorderThickness="3,2,0,0" Click="tab_forward"></Button>
                    </Grid>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
